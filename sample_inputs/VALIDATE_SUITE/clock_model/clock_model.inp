# Input file

start parameters
    nItr = 20000000
    timeStep = 10 #us

    pdbWrite = 1000
    timeWrite = 1000
    trajWrite = 100000
    restartWrite = 100000
end parameters

start boundaries
    waterBox = [1612,1612,1612] #in nm
end boundaries

start molecules
    A : 0
    R : 0
    PrA : 1
    PrR : 1
    RNR : 0    # mRNA_R
    RNA : 0    # mRNA_A
end molecules

start reactions
    # BIMOLECULAR CREATION #
    # A + R -> C (1204 uM-1s-1) 
    A(a) + R(r) <-> A(a!1).R(r!1)
    onRate3Dka = 356262.9   #nm^3/us 
    offRatekb =1.0 #1/s
    sigma = 8 #nm
    coupledRxnLabel = degA #FORCE RXN NUM LISTED (rate=INF)
    norm1=[0,0,1]
    norm2=[0,0,1]
    assocAngles=[nan, nan, nan, nan, nan]


    # PrmA + A <-> PrmA_bound (602 uM-1s-1) (50 s-1)
    PrA(a) + A(a) <-> PrA(a!1).A(a!1)
    onRate3Dka = 4888.6   # 602 1/(uM * s)
    offRatekb = 244.5 
    sigma = 5

    # PrmR + A <-> PrmR_bound (602 uM-1s-1) (100 s-1)
    PrR(r) + A(a) <-> PrR(r!1).A(a!1)
    onRate3Dka = 4888.6    # 602 1/(uM * s)
    offRatekb = 489.02 
    sigma = 5

    # UNIMOLECULAR CREATION #
    # PrmA -> PrmA + mRNA_A (50 s-1)
    PrA(a) -> PrA(a) + RNA(a)
    rate = 50    # 1/s


    # PrmR -> PrmR + mRNA_R (0.01 s-1)
    PrR(r) -> PrR(r) + RNR(r)
    rate = 0.01   # 1/s


    # mRNA_A -> mRNA_A + A (50 s-1)
    RNA(a) -> RNA(a) + A(a)
    rate = 50     # 1/s


    # mRNA_R -> mRNA_R + R (5 s-1)
    RNR(r) -> RNR(r) + R(r)
    rate = 5      # 1/s


    # PrmR_bound -> PrmR_bound + mRNA_A (500 s-1)
    PrA(a!*).A(a!*) -> PrA(a!*).A(a!*) + RNA(a)
    rate = 500    # 1/s


    # PrmR_bound -> PrmR_bound + mRNA_R (50 s-1)
    PrR(r!*).A(a!*) -> PrR(r!*).A(a!*) + RNR(r)
    rate = 50    # 1/s


    # DESTRUCTION #
    # mRNA_A -> NULL (10 s-1)
    RNA(a) -> null
    rate = 10     # 1/s


    # mRNA_R -> NULL (0.5 s-1)
    RNR(r) -> null
    rate = 0.5    # 1/s


    # A -> NULL (1 s-1)
    A(a) -> null
    rate = 1      # 1/s
    rxnLabel = degA


    
    # R -> NULL (0.2 s-1)
    R(r) -> null
    rate = 0.2    # 1/s

end reactions
